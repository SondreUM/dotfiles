{
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.cursorBlinking": "solid",
    "git.suggestSmartCommit": false,
    "C_Cpp.errorSquiggles": "enabled",
    "C_Cpp.clang_format_style": "file",
    "workbench.editorAssociations": {
        "*.given": "hexEditor.hexedit",
        "*.img": "hexEditor.hexedit",
        "*.bin": "hexEditor.hexedit",
        "*.hex": "hexEditor.hexedit",
        "*.db": "default",
        "*.pdf": "latex-workshop-pdf-hook"
    },
    "git.confirmSync": false,
    "hexeditor.columnWidth": 16,
    "hexeditor.showDecodedText": true,
    "explorer.confirmDragAndDrop": false,
    "latex-workshop.view.pdf.viewer": "tab",
    "telemetry.telemetryLevel": "off",
    "C_Cpp.intelliSenseEngine": "disabled",
    "clangd.path": "/home/sondre/.config/Code/User/globalStorage/llvm-vs-code-extensions.vscode-clangd/install/17.0.3/clangd_17.0.3/bin/clangd",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "Ubuntu (WSL)": {
            "path": "C:\\Windows\\System32\\wsl.exe",
            "args": [
                "-d",
                "Ubuntu"
            ]
        }
    },
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "settingsSync.ignoredExtensions": [
        "ms-vscode-remote.remote-wsl",
        "ms-vsliveshare.vsliveshare"
    ],
    "github.copilot.enable": {
        "*": true,
        "yaml": true,
        "plaintext": false,
        "markdown": true
    },
    "doxdocgen.generic.authorEmail": "https://github.com/SondreUM",
    "doxdocgen.generic.authorName": "SUM",
    "doxdocgen.generic.dateFormat": "YYYY-MM-DD",
    "rust-analyzer.completion.callable.snippets": "add_parentheses",
    "rust-analyzer.completion.postfix.enable": false,
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "editor.inlineSuggest.enabled": true,
    "workbench.startupEditor": "none",
    "rust-analyzer.check.command": "clippy",
    "git.autofetch": true,
    "[c]": {
        "editor.tabSize": 4,
        "editor.indentSize": "tabSize",
        "editor.insertSpaces": true,
        "editor.bracketPairColorization.enabled": true,
        "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd",
    },
    "black-formatter.args": [
        "--line-length",
        "79",
        "--target-version",
        "py311",
    ],
    "isort.args": [
        "--profile",
        "black",
        "--line-length",
        "79",
    ],
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
    },
    "python.analysis.autoFormatStrings": true,
    "python.analysis.completeFunctionParens": true,
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.pytestParameters": true,
    "python.analysis.typeCheckingMode": "standard",
    "python.analysis.inlayHints.variableTypes": true,
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer"
    },
    "files.autoSave": "onFocusChange",
    "editor.rulers": [
        79
    ],
    "editor.tabCompletion": "on",
    "C_Cpp.autocompleteAddParentheses": true,
    "explorer.confirmDelete": false,
    "C_Cpp.clang_format_sortIncludes": true,
    "C_Cpp.formatting": "clangFormat",
    "C_Cpp.vcFormat.indent.braces": true,
    "editor.tabSize": 4,
    "editor.indentSize": "tabSize",
    "editor.insertSpaces": true,
    "python.analysis.inlayHints.callArgumentNames": "all",
    "files.associations": {
        ".clang-format": "plaintext"
    },
    "editor.cursorStyle": "block",
    "github.copilot.advanced": {},
    "[xml]": {
        "editor.defaultFormatter": "DotJoshJohnson.xml"
    },
    "update.showReleaseNotes": false,
    "window.zoomLevel": 1,
    "isort.check": true,
    "latex-workshop.bibtex-format.sort.enabled": true,
    "latex-workshop.bibtex-format.trailingComma": true,
    "latex-workshop.intellisense.package.dirs": [
        "styles"
    ],
    "latex-workshop.latex.autoBuild.run": "onSave",
    "latex-workshop.latex.autoBuild.interval": 2000,
    "latex-workshop.latex.clean.subfolder.enabled": true,
    "workbench.settings.applyToAllProfiles": [],
    "python.analysis.autoImportCompletions": true,
    "C_Cpp.clang_format_fallbackStyle": "Microsoft",
    "[markdown]": {
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
    },
    "editor.formatOnSave": true,
    "json.schemas": [],
    "github.copilot.editor.enableAutoCompletions": true,
    "python.experiments.enabled": false,
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
}